{
  "name": "ibantools",
  "version": "3.3.0",
  "description": "Validation, extraction and creation of IBAN, BBAN, BIC/SWIFT numbers plus some other helpful stuff like ISO 3136-1 alpha 2 country list",
  "keywords": [
    "IBAN",
    "BBAN",
    "BIC",
    "SEPA",
    "SWIFT",
    "ISO 3136-1 alpha-2"
  ],
  "homepage": "https://github.com/Simplify/ibantools",
  "bugs": "https://github.com/Simplify/ibantools/issues",
  "main": "build/ibantools.js",
  "module": "jsnext/ibantools.js",
  "jspm": {
    "main": "jsnext/ibantools.js",
    "format": "esm"
  },
  "files": [
    "build",
    "jsnext",
    "LICENSE",
    "typings.json"
  ],
  "directories": {
    "doc": "docs"
  },
  "types": "build/ibantools.d.ts",
  "typescript": {
    "definition": "build/ibantools.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Simplify/ibantools.git"
  },
  "scripts": {
    "build": "rm -rf dist && rm -rf jsnext && rm -rf build && gulp all",
    "test": "gulp test-it",
    "karma": "gulp karma",
    "coverage": "gulp coverage",
    "lint": "gulp lint",
    "prepare": "npm run build",
    "docs": "gulp docs"
  },
  "author": {
    "name": "Saša Jovanić",
    "url": "https://www.simplify.ba/"
  },
  "license": "MPL-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "chai": "^4.2.0",
    "cheerio": "^0.22",
    "coveralls": "^3.1.0",
    "docdash": "^1.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-mocha": "^8.0",
    "gulp-rename": "^2.0",
    "gulp-shell": "^0.8.0",
    "gulp-typescript": "^5.0",
    "jasmine-core": "^3.6.0",
    "karma": "^6.1",
    "karma-chrome-launcher": "^3.1",
    "karma-jasmine": "^4.0",
    "karma-requirejs": "^1.1",
    "merge2": "^1.4.1",
    "mocha": "^8.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "requirejs": "^2.3.6",
    "typedoc": "^0.20.30",
    "typescript": "^4.0"
  }
}
